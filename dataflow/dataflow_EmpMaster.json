{
	"name": "dataflow_EmpMaster",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "EmpMaster",
						"type": "DatasetReference"
					},
					"name": "sourceEMPMD"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "dbo_NYC_Payroll_EMP_MD",
						"type": "DatasetReference"
					},
					"name": "sinkEMPMD"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          EmployeeID as string,",
				"          LastName as string,",
				"          FirstName as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     inferDriftedColumnTypes: true,",
				"     ignoreNoFilesFound: false) ~> sourceEMPMD",
				"sourceEMPMD sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          AgencyID as string,",
				"          AgencyName as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     truncate:true,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> sinkEMPMD"
			]
		}
	}
}